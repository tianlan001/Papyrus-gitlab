<plugin>

    <extension
         point="org.eclipse.m2m.qvt.oml.javaBlackboxUnits">
      <unit
            description="This Blackbox provides useful emf methods"
            name="EcoreHelper"
            namespace="org.eclipse.papyrus.uml.m2m.qvto.common.blackboxes.ecore">
         <library
               class="org.eclipse.papyrus.uml.m2m.qvto.common.blackboxes.ecore.EcoreHelper"
               name="EcoreHelper">
            <metamodel
                  nsURI="http://www.eclipse.org/emf/2002/Ecore">
            </metamodel>
         </library>
      </unit>
      <unit
            description="This Blackbox provides useful uml methods"
            name="UMLHelper"
            namespace="org.eclipse.papyrus.uml.m2m.qvto.common.blackboxes.uml">
         <library
               class="org.eclipse.papyrus.uml.m2m.qvto.common.blackboxes.uml.UMLHelper"
               name="UMLHelper">
            <metamodel
                  nsURI="http://www.eclipse.org/uml2/5.0.0/UML">
            </metamodel>
         </library>
      </unit>
      <unit
            description="Provides some EMF Resource manipulation helpers"
            name="EMFResource"
            namespace="org.eclipse.papyrus.uml.m2m.qvto.common.blackboxes.emf">
         <library
               class="org.eclipse.papyrus.uml.m2m.qvto.common.blackboxes.emf.EMFResourceUtils"
               name="EMFResourceUtils">
            <metamodel
                  nsURI="http://www.eclipse.org/emf/2002/Ecore">
            </metamodel>
         </library>
      </unit>
      <unit
            description="Provides an access to Config Properties"
            name="Properties"
            namespace="org.eclipse.papyrus.uml.m2m.qvto.common.blackboxes.utils">
         <library
               class="org.eclipse.papyrus.uml.m2m.qvto.common.blackboxes.utils.PropertiesUtil"
               name="PropertiesUtil">
            <metamodel
                  nsURI="http://www.eclipse.org/emf/2002/Ecore">
            </metamodel>
         </library>
      </unit>
    </extension>
</plugin>
